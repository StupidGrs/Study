{
  "name": "mercer-testframework-investments-src",
  "version": "3.0.2",
  "description": "Mercer end-to-end test automation scripts",
  "keywords": [
    "Angular",
    "Protractor",
    "Cucumber"
  ],
  "author": "Mercer",
  "repository": "https://bitbucket.org/oliverwymantechssg/ngpd-merceros-testautomation-template",
  "license": "GPL-3.0",
  "dependencies": {
    "ngpd-merceros-testautomation-ta": ">=3.0.2",
    "protractor": "5.3.1",
    "chai": "4.1.2",
    "pixelmatch": "4.0.2",
    "pngjs": "3.3.3",
    "chai-as-promised": "7.1.1",
    "cucumber": "4.1.0",
    "protractor-cucumber-framework": "5.0.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.7.0",
    "jsonpath": "1.0.0",
    "yargs": "^11.0.0",
    "uuid": "3.3.3",
    "request-promise": "4.2.4",
    "https-proxy-agent": "^3.0.1" 
  },
  "scripts": {
    "configure-globals": "npm run config/configure-globals.js",
    "prestart-webdriver": "webdriver-manager update",
    "start-webdriver": "webdriver-manager start",
    "start-webdriver-ie": "webdriver-manager start --ie",
    "start-webdriver-edge": "webdriver-manager start --edge C:\\MicrosoftWebDriver.exe",
    "start-webdriver-jenkins": "bash run-e2e-tests.sh",
    "test-local": "protractor protractor.local.conf.js",
    "test-smoke": "protractor protractor.smoke.conf.js",
    "test-aws": "protractor protractor.conf.aws.js",
    "updateScreenshots": "node ./updateScreenshots.js all",
    "test-regression": "protractor protractor.regression.conf.js",
    "test-regression-local-chrome": "TEST_ENV=LOCAL BROWSER_NAME=chrome MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-local-ff": "TEST_ENV=LOCAL BROWSER_NAME=firefox MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-local-ie": "TEST_ENV=LOCAL BROWSER_NAME=\"internet explorer\" MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-local-edge": "TEST_ENV=LOCAL BROWSER_NAME=MicrosoftEdge MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-dev-chrome": "TEST_ENV=AWS_DEV BROWSER_NAME=chrome MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-dev-ff": "TEST_ENV=AWS_DEV BROWSER_NAME=firefox MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-stage-chrome": "TEST_ENV=AWS_STAGE BROWSER_NAME=chrome MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-stage-ff": "TEST_ENV=AWS_STAGE BROWSER_NAME=firefox MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-stage-edge": "TEST_ENV=STAGE BROWSER_NAME=MicrosoftEdge MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-stage-ie": "TEST_ENV=STAGE BROWSER_NAME=\"internet explorer\" MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-regression-stage-edge-sauce": "TEST_ENV=STAGE BROWSER_NAME=MicrosoftEdge MAX_INSTANCES=10 protractor protractor.sauce.conf.js",
    "test-regression-stage-ie-sauce": "TEST_ENV=STAGE BROWSER_NAME=\"internet explorer\" MAX_INSTANCES=10 protractor protractor.sauce.conf.js",
    "test-e2e": "protractor protractor.e2e.conf.js",
    "test-e2e-local-chrome": "TEST_ENV=LOCAL BROWSER_NAME=chrome MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-local-ff": "TEST_ENV=LOCAL BROWSER_NAME=firefox MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-local-ie": "TEST_ENV=LOCAL BROWSER_NAME=\"internet explorer\" MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-local-edge": "TEST_ENV=LOCAL BROWSER_NAME=MicrosoftEdge pMAX_INSTANCES=10 rotractor protractor.e2e.conf.js",
    "test-e2e-dev-chrome": "TEST_ENV=AWS_DEV BROWSER_NAME=chrome MAX_INSTANCES=10 protractor protractor.regression.conf.js",
    "test-e2e-dev-ff": "TEST_ENV=AWS_DEV BROWSER_NAME=firefox protractor protractor.smoke.conf.js",
    "test-e2e-stage-chrome": "TEST_ENV=AWS_STAGE BROWSER_NAME=chrome MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-stage-ff": "TEST_ENV=AWS_STAGE BROWSER_NAME=firefox MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-stage-edge": "TEST_ENV=STAGE BROWSER_NAME=MicrosoftEdge MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-stage-ie": "TEST_ENV=STAGE BROWSER_NAME=\"internet explorer\" MAX_INSTANCES=10 protractor protractor.e2e.conf.js",
    "test-e2e-stage-edge-sauce": "TEST_ENV=STAGE BROWSER_NAME=MicrosoftEdge MAX_INSTANCES=10 protractor protractor.e2e.sauce.conf.js",
    "test-e2e-stage-ie-sauce": "TEST_ENV=STAGE BROWSER_NAME=\"internet explorer\" MAX_INSTANCES=10 protractor protractor.e2e.sauce.conf.js",
    "cleanup": "node -e 'require(\"./cleanup.js\").cleanUpTestData()'"
  }
}
